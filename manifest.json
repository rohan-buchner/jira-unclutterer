{
  "manifest_version": 2,
  "name": "jira_unclutterer",
  "version": "0.1",
  "content_scripts": [{
    "matches": [
      "https://oneafricamedia.atlassian.net/*"
    ],
    	"js": ["content.js"]
  	}
  ],
  "browser_action": {
    "default_icon": "icon16.png"
  },
  "icons": { 
  	"16": "icon16.png",
  	"48": "icon48.png",
  	"128": "icon128.png"
  }
}